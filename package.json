{
  "name": "@pas7/nestjs-strict-json",
  "version": "0.2.2",
  "description": "Strict JSON parsing for NestJS (Express/Fastify) with duplicate-key detection",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pas7-studio/nestjs-strict-json.git"
  },
  "homepage": "https://github.com/pas7-studio/nestjs-strict-json",
  "bugs": {
    "url": "https://github.com/pas7-studio/nestjs-strict-json/issues"
  },
  "keywords": [
    "nestjs",
    "strict-json",
    "duplicate-keys",
    "fastify",
    "express",
    "security"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "bench:parser": "npm run build && node scripts/benchmark-parser.mjs",
    "lint": "eslint .",
    "test": "vitest run",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:watch": "vitest",
    "typecheck": "tsc -p tsconfig.build.json --noEmit"
  },
  "dependencies": {
    "jsonc-parser": "3.3.1"
  },
  "peerDependencies": {
    "@nestjs/common": ">=10",
    "@nestjs/core": ">=10"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "@nestjs/core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nestjs/common": "^10.4.0",
    "@nestjs/core": "^10.4.0",
    "@nestjs/platform-express": "^10.4.0",
    "@nestjs/platform-fastify": "^10.4.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "express": "^4.19.2",
    "fastify": "^4.26.0",
    "tsup": "^8.0.2",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  }
}
